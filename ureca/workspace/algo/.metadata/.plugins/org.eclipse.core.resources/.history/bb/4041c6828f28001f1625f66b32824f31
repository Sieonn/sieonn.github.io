package day0612;

import java.util.ArrayList;

public class OverridingTest {

	public static void main(String[] args) {
		
		//가변배열
		ArrayList list1 = new ArrayList();//데이터를 입력을하면 사이즈가 알아서 할당된다.
			list1.add("홍길동");
			list1.add("길라임");
			list1.add("김주원");
			list1.add(1004); //1004가 그냥 들어가는게 아니라 new Integer(1004);로 적용되는것
			
			
	}

}
