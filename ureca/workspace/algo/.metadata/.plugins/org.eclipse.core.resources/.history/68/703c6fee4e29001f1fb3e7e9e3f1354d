package com.ureca;

class A {
	public void happy() {
		System.out.println("즐거우세요? 아 즐거우시냐고요ㅎ");
	}
}//class A

class Child extends A{
	@Override
	public void happy() {
		System.out.println("행복 찾기(ﾉ◕ヮ◕)ﾉ*:･ﾟ✧");
	}
}
public class AnonymousInnerClassTest {

	public static void main(String[] args) {
		A a = new Child();//new A();
		a.happy();
	}

}


