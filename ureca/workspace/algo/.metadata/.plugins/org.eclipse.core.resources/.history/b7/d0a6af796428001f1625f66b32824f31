package com.ureca;

public class NameMenu {
	// 데이터에 대한 CRUD

	// 데이터 저장소
	String[] names = new String[5];

	void create(String name) {
		for (int i = 0; i < names.length; i++) {
			if (names[i] == null){//빈곳 찾기
				names[i] = name;
			}
		}
	}

	void read() {

	}

	void update() {

	}

	void delete() {

	}
}
