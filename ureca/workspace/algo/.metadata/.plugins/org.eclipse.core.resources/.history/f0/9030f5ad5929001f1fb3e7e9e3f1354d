package Mission;

public class Person implements Comparable<>{
	String name;
	int age;
	String job;
	int year;
	
	public Person(String name, int age, String job, int year) {
		this.name=name;
		this.age=age;
		this.job=job;
		this.year=year;
	}

	public int compareTo(Person o) {
		if(this.name>o.name) return 1;
		return 0;
	}

}
